{
  "name": "insight-sdk",
  "version": "1.1.9",
  "description": "A React SDK for displaying insights with interactive charts and data visualizations",
  "author": "Insight SDK Team",
  "license": "ISC",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": ["**/*.css"],
  "scripts": {
    "build": "rollup -c",
    "dev": "http-server . -p 3000 -c-1"
  },
  "keywords": [
    "react",
    "react-sdk",
    "sdk",
    "insights",
    "charts",
    "data-visualization",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/insight-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/insight-sdk/issues"
  },
  "homepage": "https://github.com/your-username/insight-sdk#readme",
  "peerDependencies": {
    "react": ">=16.8 <20",
    "react-dom": ">=16.8 <20"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "http-server": "^14.1.1",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.6.0",
    "rollup": "^4.53.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.9.3"
  }
}
